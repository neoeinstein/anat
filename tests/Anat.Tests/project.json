{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "Anat.Tests.fs",
        "Anat.Examples.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc":"1.0.0-preview2-*"
  },
  "testRunner": "xunit",
  "dependencies": {
    "Anat": {
      "target": "project"
    },
    "FSharp.Core": "4.0.1.5",
    "Unquote": "3.1.2",
    "dotnet-test-xunit": "2.2.0-preview2-*",
    "xunit": "2.2.0-beta*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": ""
      },
      "imports": [
        "portable-net45+win8+wp8+wpa81"
      ]
    }
  }
}